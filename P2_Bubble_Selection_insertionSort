#include <stdio.h>
int main()
{
    int s;
    int arr[5] = {5,7,2,9,12};
    printf("Press\n1:Insertion Sort\n2:Bubble Sort\n");
    scanf("%d",&s);
    switch(s)
    {
        case 1:
        {
            int j,key,i;
            for(j=1;j<sizeof(arr)/4;j++){  // 
                key = arr[j]; //2
                i = j-1; //1
                while(i>=0 && arr[i]>key){ //T & 7>2 
                arr[i+1] = arr[i];  //7,5,2,9,12
                i =i-1;
                }
                arr[i+1] = key;
        }
        for(j = 0;j<sizeof(arr)/4;j++){
            printf("%d ",arr[j]);
        }     
        break;
    }
    case 2:
    {
        int i,j,temp;
        for(j = 0 ; j< sizeof(arr)/4;j++){  //
        for(i = 0;i<(sizeof(arr)/4)-1;i++){
            if(arr[i]>arr[i+1]){
                temp = arr[i];
                arr[i] = arr[i+1];
                arr[i+1] = temp;
            }
        }
    }
    for(j = 0 ; j< sizeof(arr)/4;j++){
        printf("%d ",arr[j]);
    }
        break;
    }
    default:
    {
        printf("INVALID CHOICE");
    }
    }
    return 0;
}
